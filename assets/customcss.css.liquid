.aurthor-container .aurthor-info { flex: 1; }
.aurthor-container .aurthor-info .title { font-size: var(--font-sm); font-weight: 600; font-family: var(--primaryFont); margin: 0; color: var(--primary); letter-spacing:0; text-transform:capitalize; }
.aurthor-container .aurthor-info .aurthor-location { font-size: var(--lead-14); font-family: var(--secondaryFont); text-transform: uppercase; color: rgba(var(--secondaryrgb), 0.4); font-weight:700; }
.testimonial_section > button { bottom: 0 !important; top: auto !important; width: 50px !important; height: 50px !important; border: 1px solid rgba(var(--secondaryrgb), 0.1); border-radius: 50%; --swiper-navigation-size: 16px; color: var(--secondary); right: 0 !important; z-index: 99; transform: translate(0px); display:flex; align-items:center; justify-content:center; }
.testimonial_section > button.slick-prev { left: auto !important; right: 70px !important; }
.testimonial_section > button:before { color: var(--primery); opacity: 1; display: inline-block; mask: url(/cdn/shop/files/slider-arrow.svg?v=1714030341); mask-repeat: no-repeat; mask-size: cover; background-color: currentColor; mask-size: 10px auto; width: 10px; height: 18px;  }
